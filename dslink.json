{
  "name": "dslink-java-mango",
  "version": "1.3",
  "description": "A Java DSLink for Mango",
  "license": "Apache",
  "author": {
    "name": "Peter Weise",
    "email": "p.weise@dglogik.com"
  },
  "main": "bin/dslink-java-mango",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-java-mango"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-java-mango/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "mango"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "org.dsa.iot.mango.Main"
    }
  }
}
